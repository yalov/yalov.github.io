DTSTART;VALUE=DATE:20180728
DTSTAMP:20181231T220111Z
UID:20181231T230111CET-3153unxUhA@calend.ru/russtate/
RRULE:FREQ=YEARLY;BYDAY=4SA;BYMONTH=7
