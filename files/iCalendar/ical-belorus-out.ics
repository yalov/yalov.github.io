BEGIN:VEVENT
SUMMARY:День всенародной памяти жертв Велик
 ой Отечественной войны в Беларуси
DTSTART;VALUE=DATE:20220622
DTSTAMP:20220101T010103Z
UID:20220101T020103CET-6922uIuiiw@calend.ru/belorus/
RRULE:FREQ=YEARLY
